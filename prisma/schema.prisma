// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Patients {
  id              Int    @id @default(autoincrement())
  name            String
  phoneNumber     String
  bloodGroup      String
  date            String
  time            String
  reservationType String
}
